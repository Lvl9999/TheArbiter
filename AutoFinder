-- getgenv().AutoFindingArbiter = true;

repeat task.wait(0.1) until game:IsLoaded();
local BoredLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/Lvl9999/Bored/main/Backup"))();

local ServerList = 11805362789;
local SakuraStand = 8534845015;

task.spawn(function()
    while getgenv().AutoFindingArbiter == true do
        pcall(function()
            if game.PlaceId == ServerList then
                local Frames = {}
                for _,x in ipairs(game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui"):WaitForChild("ServerListGui"):WaitForChild("List"):GetChildren()) do
                    if x:IsA("Frame") then
                        table.insert(Frames,x);
                    end
                end

                if #Frames > 0 then
                    local Selected = Frames[math.random(#Frames)]
                    BoredLibrary.prompt("Sakura Hub    ðŸŒ¸","Teleporting to: "..Selected.Name,0.35);
                    local Sound = Instance.new("Sound");Sound.Name = "Sound";Sound.SoundId = "http://www.roblox.com/asset/?id=289556450";
                    Sound.Volume = 10;Sound.Looped = false;Sound.Archivable = false;Sound.Parent = game.Workspace;Sound:Play();task.wait(0.1);
                    local args = {[1] = Selected.Name};
                    game:GetService("ReplicatedStorage"):WaitForChild("TP"):FireServer(unpack(args));
                end
            elseif game.PlaceId == SakuraStand then
                if not workspace.Living:FindFirstChild("Arbiter") or not workspace.Living["Arbiter"]:FindFirstChild("Head") then
                    BoredLibrary.prompt("Sakura Hub    ðŸŒ¸","Arbiter Is Not Here...",0.75);BoredLibrary.prompt("Sakura Hub    ðŸŒ¸","Teleporting to ServerList...",0.5);
                    local Sound = Instance.new("Sound");Sound.Name = "Sound";Sound.SoundId = "http://www.roblox.com/asset/?id=289556450";
                    Sound.Volume = 10;Sound.Looped = false;Sound.Archivable = false;Sound.Parent = game.Workspace;Sound:Play();task.wait(0.1);
                    game:GetService("TeleportService"):Teleport(ServerList,game.Players.LocalPlayer);
                else
                    getgenv().AutoFindingArbiter = false;
                    local Sound = Instance.new("Sound");Sound.Name = "Sound";Sound.SoundId = "http://www.roblox.com/asset/?id=8097518145";
                    Sound.Volume = 10;Sound.Looped = true;Sound.Archivable = false;Sound.Parent = game.Workspace;Sound:Play();
                    BoredLibrary.prompt("Sakura Hub    ðŸŒ¸","Arbiter Is Here!");
                end
            end
        end)
        task.wait(1.75);
    end
end)
